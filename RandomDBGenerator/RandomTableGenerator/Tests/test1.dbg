table User(
  Id integer [1,50000],
  Email varchar [5,30],
  Username varchar [5,30],
  Balance real [0.0,1500.0],
  CreditCardNo text 16;
  primary key (Id)
) with rows 1000;

table Game(
  Title varchar [5,50],
  Year integer [1980,2019],
  Released boolean,
  Price real [1.0,100.0],
  PEGI integer [0,18];
  primary key(Title,Year)
) with rows 500;

table User_Game(
  UserId integer [1,50000],
  Title varchar [5,50],
  Year integer [1980,2019];
  foreign key (UserId) references User,
  foreign key (Title,Year) references Game;
  primary key (UserId,Title,Year)
) with rows 250